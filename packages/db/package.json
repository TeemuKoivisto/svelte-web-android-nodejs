    "format": "prisma format",
    "reset": "prisma migrate reset && tsx ./prisma/seed.ts && tsx ./prisma/zod-schemas.ts",
    "seed": "tsx ./prisma/seed.ts",
    "zod": "tsx ./prisma/zod-schemas.ts"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "dotenv": "^17.2.1",
    "prisma": "^6.12.0",
    "tsx": "^4.20.3",
    "zod-prisma-types": "^3.2.4"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0"
  }
}