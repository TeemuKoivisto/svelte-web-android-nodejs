{
  "name": "@org/db",
  "version": "0.0.1",
  "private": true,
  "main": "generated/client.ts",
  "types": "generated/client.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./generated/index.js"
    },
    "./client": {
      "import": "./generated/client.ts"
    }
  },
  "scripts": {
    "prod:migrate": "prisma migrate deploy",
